<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.urosjarc.dbanalyser.gui.widgets.ColumnSearch">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
   <children>
      <TextField fx:id="nameTF" alignment="CENTER" promptText="Column name" />
      <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="20.0" VBox.vgrow="NEVER">
         <CheckBox fx:id="primaryKeyCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Primary key" />
         <CheckBox fx:id="foreignKeyCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Foreign key" />
         <CheckBox fx:id="selfRefKeyCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Self referenced key" />
      </HBox>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" spacing="20.0">
         <CheckBox fx:id="uniqueCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Unique" />
         <CheckBox fx:id="notNullCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Not null" />
         <CheckBox fx:id="defaultCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Default" />
         <CheckBox fx:id="constrainedCB" allowIndeterminate="true" indeterminate="true" mnemonicParsing="false" text="Constrained" />
      </HBox>
      <Separator maxWidth="1.7976931348623157E308" />
      <FlowPane fx:id="typeFP" alignment="CENTER" columnHalignment="CENTER" hgap="20.0" vgap="5.0" />
      <Button fx:id="searchB" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Search" />
       <fx:include fx:id="columnTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="../parts/ColumnTableView.fxml" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
