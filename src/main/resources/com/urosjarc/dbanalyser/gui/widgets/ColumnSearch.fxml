<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.urosjarc.dbanalyser.gui.widgets.ColumnSearch">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
    <children>
        <TextField fx:id="nameTF" alignment="CENTER" promptText="Column name" />
        <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" spacing="20.0" VBox.vgrow="NEVER">
            <CheckBox fx:id="primaryKeyCB" mnemonicParsing="false" selected="true" text="Primary key" />
            <CheckBox fx:id="foreignKeyCB" mnemonicParsing="false" selected="true" text="Foreign key" />
            <CheckBox fx:id="selfRefKeyCB" mnemonicParsing="false" selected="true" text="Self referenced key" />
        </HBox>
        <Separator prefWidth="200.0" />
        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" spacing="20.0">
            <CheckBox fx:id="uniqueCB" mnemonicParsing="false" selected="true" text="Unique" />
            <CheckBox fx:id="notNullCB" mnemonicParsing="false" selected="true" text="Not null" />
            <CheckBox fx:id="defaultCB" mnemonicParsing="false" selected="true" text="Default" />
            <CheckBox fx:id="constrainedCB" mnemonicParsing="false" selected="true" text="Constrained" />
        </HBox>
        <Separator maxWidth="1.7976931348623157E308" />
        <FlowPane fx:id="typeFP" alignment="CENTER" columnHalignment="CENTER" hgap="20.0" vgap="5.0" />
      <HBox spacing="5.0">
         <children>
            <Button fx:id="selectAllB" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Select all" HBox.hgrow="ALWAYS" />
            <Button fx:id="deselectAllB" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Deselect all" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
        <fx:include fx:id="columnTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="../parts/ColumnTableView.fxml" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
